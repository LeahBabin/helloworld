#code for population, etc over the years
x <- read.csv("lyme2yearlag.csv")
library(dplyr)
x97<-filter(x, yr == 1997)
x97<-mutate(x97,LD = ir3 * ppsm * sqmi)
sum(x97$LD)/10000
sum(x97$Deer)
cor(x97$hk, x97$ir3)
x97<- mutate(x97, pop = ppsm * sqmi)
sum(x97$pop)/sum(x97$sqmi)
x97<- mutate(x97, pov = upl * pop)
sum(x97$pov)/sum(x97$pop)
x98<- filter(x, yr == 1998)
